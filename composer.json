{
    "name": "beastbytes/yii-leaflet",
    "description": "Leaflet JavaScript mapping library integration for Yii3",
    "type": "extension",
    "keywords": [
        "Leaflet",
        "map",
        "mapping",
        "maps",
        "yii3"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Chris Yates",
            "email": "chris.l.yates@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "yiisoft/assets": "^5.0",
        "yiisoft/html": "^3.0",
        "yiisoft/widget": "^2.0"
    },
    "require-dev": {
        "consolidation/robo": "^4.0",
        "phpunit/phpunit": "^10.0",
        "roave/infection-static-analysis-plugin": "^1.0",
        "roave/security-advisories": "dev-latest",
        "yiisoft/aliases": "^3.0",
        "yiisoft/cache": "^3.0",
        "yiisoft/event-dispatcher": "^1.0",
        "yiisoft/test-support": "^3.0",
        "yiisoft/view": "^12.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Widgets\\Leaflet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\Widgets\\Leaflet\\Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "robo": "robo",
        "test": "phpunit --testdox"
    }
}
